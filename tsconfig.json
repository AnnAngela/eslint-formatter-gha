{
    "compilerOptions": {
        "target": "ES2022", // Node 18
        "lib": [
            "ES2023", // Node 18
        ],
        "module": "Node16", // Node 18
        "moduleResolution": "Node16", // Node 18
        "removeComments": true,
        "outDir": ".cache/dist/",
        "alwaysStrict": true,
        "strict": true,
        "downlevelIteration": true,
        "rootDir": "./src",
        "baseUrl": "./",
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "newLine": "lf",
        "noEmit": true,
    },
    "include": [
        "src",
    ],
    "exclude": [
        "dist",
        "node_modules",
    ],
    "$schema": "https://json.schemastore.org/tsconfig",
}
